   "outputs": [],
   "source": [
    "# Read each of the csvs to dataframe and combine all into one large dataframe with the dates in a column\n",
    "\n",
    "# Create a list to hold all of the dataframe objects (note: different from the file paths)\n",
    "all_frames = []\n",
    "\n",
    "# each file path in the list gets read to a dataframe\n",
    "for item in all_csv: \n",
    "    temp_frame = pd.read_csv(item)\n",
    "\n",
    "    # get the date/time created from file itself (rather than reading the folder name to get the timestamp)\n",
    "    date_str = time.ctime(os.path.getctime(item))\n",
    "    # create a column for the date-time data\n",
    "    temp_frame['date_created'] = pd.to_datetime(date_str)\n",
    "\n",
    "    # add the frame to the list\n",
    "    all_frames.append(temp_frame)\n",
    "\n",
    "\n",
    "# All of the data frames in one massive data frame- dates included\n",
    "all_data = pd.concat(all_frames, ignore_index=True)\n",
    "\n",
    "# Note: The \"all_data\" dataframe could be huge. I do not recommend outputting it to a CSV or using the print command prior to filitering for specific values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "957faa09-b608-43cb-8a9a-157658f20162",
   "metadata": {},
   "outputs": [],
   "source": [
    "# PLOT\n",
    "# only number of fallouts by date\n",
    "\n",
    "# Set variable to hold the number of total entries by date\n",
    "date_counts = all_data['date_created'].value_counts().sort_index()\n",
    "\n",
    "# Use matplotlib to create a line graph of the number of fallouts per date of report\n",
    "fig, ax = plt.subplots()\n",
    "\n",
    "# Set the x axis, then y axis\n",
    "ax.plot(date_counts.index, date_counts.values)\n",
    "\n",
    "# Axis labels, figure title\n",
    "ax.set_xlabel('Date')\n",
    "ax.set_ylabel('Count')\n",
    "ax.set_title('Fallouts by Date')\n",
    "\n",
    "# Format the x axis dates, rotate labels for better fit\n",
    "ax.xaxis.set_major_formatter(mdates.DateFormatter('%Y-%m-%d'))\n",
    "plt.xticks(rotation=45, ha='right') \n",
    "\n",
    "# Adjust the layout as desired\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
