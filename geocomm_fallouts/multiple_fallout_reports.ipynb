    "# REQUIRED\n",
    "import os\n",
    "import pandas as pd\n",
    "import csv\n",
    "from pathlib import Path\n",
    "import time\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import matplotlib.dates as mdates \n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "57e978ed-b7be-4806-8eaa-90f1ff9af682",
   "metadata": {},
   "outputs": [],
   "source": [
    "# FUNCTION\n",
    "# Get the csv file path from each sub directory and add to a list. \n",
    "# Ignores the .gdb files also in the folder\n",
    "\n",
    "def getFilesFromDir(root_dir):\n",
    "    # array of the reports\n",
    "    all_reports = []\n",
    "    # Go through all the directories in the root directory\n",
    "    for dirpath, dirnames, filenames in os.walk(root_dir):\n",
    "        for filename in filenames: \n",
    "            # Get the file extensions for the filenames in the directory\n",
    "            name, ext = os.path.splitext(filename)\n",
    "            # specify the extension to filter for csv only, put the full file path in the array to call later\n",
    "            if ext == '.csv':\n",
    "                full_file_path = os.path.join(dirpath, filename)\n",
    "                all_reports.append(full_file_path)\n",
    "    return all_reports\n",
    "\n"
   ]
  },
